<%= form_for @post, url: { action: 'create'} do |f|%>
    <%= f.text_field :title, :required => true %>
    <%= f.text_field :content, :minlength => 30 %>
    <%= f.collection_select :blogger_id, Blogger.all, :id, :name %>
    <%= f.collection_select :destination_id, Destination.all, :id, :name %>
    <%= f.submit %>
<% end %>